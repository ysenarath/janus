{
  "manifest_version": 3,
  "name": "Custom New Tab",
  "version": "1.0",
  "description": "A custom new tab page extension built with React and Tailwind CSS",
  "chrome_url_overrides": {
    "newtab": "index.html"
  },
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "permissions": [
    "audioCapture"
  ],
  "content_security_policy": {
    "extension_pages": "script-src 'self' 'wasm-eval' 'wasm-unsafe-eval'; object-src 'self'",
    "worker_src": "'self' blob: 'wasm-eval' 'wasm-unsafe-eval'"
  }
}
